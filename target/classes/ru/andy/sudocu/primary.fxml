<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox fx:id="primaryVBox" alignment="TOP_LEFT" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.andy.sudocu.PrimaryController">
   <children>
      <HBox alignment="TOP_CENTER">
         <Text fx:id="sudokuHeader" id="sudokuHeader" text="Судоку"/>
      </HBox>
      <Button fx:id="primaryButton" id="primaryButton" text="Сгенерировать таблицу судоку" onAction="#calculateSudokuItems"/>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
